{
  "name": "svatkobot",
  "private": true,
  "scripts": {
    "develop": "parcel src/index.html --out-dir /app/build-dev --cache-dir /app/cache --no-source-maps --no-hmr",
    "build": "parcel build src/index.html --out-dir /app/build-prod --no-source-maps",
    "build-netlify": "parcel build src/index.html --out-dir ./build-prod --no-source-maps"
  },
  "dependencies": {
    "js-yaml": "3.14.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.3.1",
    "preact": "10.4.4",
    "sass": "1.26.5",
    "tachyons": "4.12.0"
  },
  "staticFiles": {
    "staticPath": [
      "src/static"
    ],
    "watcherGlob": "**"
  }
}
